{
  "manifest_version": 2,

  "name": "Ad Explorer",
  "description": "This extension allows you to see data about ads",
  "version": "1.0",

  "browser_action": {
    "default_icon": "favicon.png",
    "default_popup": "popup.html",
    "default_title": "Explore some Ads!"
  },
   "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "http://*/",
    "https://*/"
  ]
}
